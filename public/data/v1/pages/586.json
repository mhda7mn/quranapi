    "ayahEn": "and when wild beasts are gathered together",
    "meta": {
      "page": 586,
      "juz": 30,
      "hizb": 59,
      "hizbQuarter": 234,
      "sajda": false
    }
  },
  {
    "surahNo": 81,
    "ayahNo": 6,
    "ayahArV1": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ",
