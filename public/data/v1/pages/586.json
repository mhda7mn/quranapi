    "meta": {
      "page": 586,
      "juz": 30,
      "hizb": 59,
      "hizbQuarter": 234,
      "sajda": false
    }
  },
  {
    "surahNo": 81,
    "ayahNo": 5,
    "ayahArV1": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ",
