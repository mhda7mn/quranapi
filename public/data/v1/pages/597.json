    "meta": {
      "page": 597,
      "juz": 30,
      "hizb": 60,
      "hizbQuarter": 239,
      "sajda": false
    }
  },
  {
    "surahNo": 96,
    "ayahNo": 13,
    "ayahArV1": "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
