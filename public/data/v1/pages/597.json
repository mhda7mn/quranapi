    "meta": {
      "page": 597,
      "juz": 30,
      "hizb": 60,
      "hizbQuarter": 239,
      "sajda": false
    }
  },
  {
    "surahNo": 96,
    "ayahNo": 14,
    "ayahArV1": "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
