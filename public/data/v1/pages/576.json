    "ayahNo": 40,
    "ayahArV1": "فِى جَنَّـٰتٍ يَتَسَآءَلُونَ",
    "ayahArV2": "في جنات يتساءلون",
    "ayahEn": "who will be in Gardens, asking one another",
    "meta": {
      "page": 576,
      "juz": 29,
      "hizb": 58,
      "hizbQuarter": 230,
      "sajda": false
    }
  },
  {
