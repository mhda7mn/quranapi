    "ayahNo": 39,
    "ayahArV1": "إِلَّآ أَصْحَـٰبَ ٱلْيَمِينِ",
    "ayahArV2": "الا اصحاب اليمين",
    "ayahEn": "except the people of the right",
    "meta": {
      "page": 576,
      "juz": 29,
      "hizb": 58,
      "hizbQuarter": 230,
      "sajda": false
    }
  },
  {
