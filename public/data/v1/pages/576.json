    "ayahArV2": "والصبح اذا اسفر",
    "ayahEn": "and the day as it breaks!",
    "meta": {
      "page": 576,
      "juz": 29,
      "hizb": 58,
      "hizbQuarter": 230,
      "sajda": false
    }
  },
  {
    "surahNo": 74,
    "ayahNo": 35,
