      "page": 526,
      "juz": 27,
      "hizb": 53,
      "hizbQuarter": 210,
      "sajda": false
    }
  },
  {
    "surahNo": 53,
    "ayahNo": 12,
    "ayahArV1": "أَفَتُمَـٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
