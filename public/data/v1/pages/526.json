      "hizbQuarter": 210,
      "sajda": false
    }
  },
  {
    "surahNo": 53,
    "ayahNo": 11,
    "ayahArV1": "مَا كَذَبَ ٱلْفُؤَادُ مَا رَأَىٰٓ",
