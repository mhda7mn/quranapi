      "juz": 27,
      "hizb": 53,
      "hizbQuarter": 210,
      "sajda": false
    }
  },
  {
    "surahNo": 53,
    "ayahNo": 7,
    "ayahArV1": "وَهُوَ بِٱلْأُفُقِ ٱلْأَعْلَىٰ",
