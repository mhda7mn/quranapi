      "hizbQuarter": 210,
      "sajda": false
    }
  },
  {
    "surahNo": 53,
    "ayahNo": 22,
    "ayahArV1": "تِلْكَ إِذًا قِسْمَةٌ ضِيزَىٰٓ",
