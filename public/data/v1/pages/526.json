      "juz": 27,
      "hizb": 53,
      "hizbQuarter": 210,
      "sajda": false
    }
  },
  {
    "surahNo": 53,
    "ayahNo": 8,
    "ayahArV1": "ثُمَّ دَنَا فَتَدَلَّىٰ",
