      "juz": 30,
      "hizb": 60,
      "hizbQuarter": 238,
      "sajda": false
    }
  },
  {
    "surahNo": 93,
    "ayahNo": 4,
    "ayahArV1": "وَلَلْـَٔاخِرَةُ خَيْرٌ لَّكَ مِنَ ٱلْأُولَىٰ",
